<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane styleClass="component-with-border" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="body.currentconfig.CurrentConfigController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="295.0" />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="170.33331298828125" minHeight="30.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" prefHeight="142.0" vgrow="ALWAYS" />
   </rowConstraints>
   <children>
      <Label fx:id="configTitle" styleClass="component-title" text="Current Machine Configuration:" GridPane.halignment="CENTER" />
      <ScrollPane fitToHeight="true" fitToWidth="true" GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
         <content>
            <FlowPane orientation="VERTICAL" prefHeight="133.0" prefWrapLength="200.0">
               <children>
                  <Label id="rotors" fx:id="rotors" styleClass="config-label" stylesheets="@currentConfig.css" text="&lt;3,4,2&gt;" />
                  <Label id="windows-characters" fx:id="windows" styleClass="config-label" stylesheets="@currentConfig.css" text="&lt;BGF&gt;" />
                  <Label id="reflector" fx:id="reflector" styleClass="config-label" stylesheets="@currentConfig.css" text="&lt;IV&gt;" />
                  <Label id="plugs" fx:id="plugs" styleClass="config-label" stylesheets="@currentConfig.css" text="&lt;A|F, F|E&gt;" />
                  <Label id="noConfigMsg" fx:id="noConfigMsg" styleClass="config-label" />
               </children>
            </FlowPane>
         </content>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </ScrollPane>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <stylesheets>
      <URL value="@../../app/app.css" />
      <URL value="@currentConfig.css" />
   </stylesheets>
</GridPane>
